set $laptopws0 "0-0"
set $laptopws1 "0-1"
set $laptopws2 "0-2"
set $laptopws3 "0-3"
set $laptopws4 "0-4"
set $laptopws5 "0-5"
set $laptopws6 "0-6"
set $laptopws7 "0-7"
set $laptopws8 "0-8"
set $laptopws9 "0-9"

set $mainws0 "1-0"
set $mainws1 "1-1"
set $mainws2 "1-2"
set $mainws3 "1-3"
set $mainws4 "1-4"
set $mainws5 "1-5"
set $mainws6 "1-6"
set $mainws7 "1-7"
set $mainws8 "1-8"
set $mainws9 "1-9"

set $sidews0 "2-0"
set $sidews1 "2-1"
set $sidews2 "2-2"
set $sidews3 "2-3"
set $sidews4 "2-4"
set $sidews5 "2-5"
set $sidews6 "2-6"
set $sidews7 "2-7"
set $sidews8 "2-8"
set $sidews9 "2-9"


workspace $laptopws0 output $laptopMonitor
workspace $laptopws1 output $laptopMonitor
workspace $laptopws2 output $laptopMonitor
workspace $laptopws3 output $laptopMonitor
workspace $laptopws4 output $laptopMonitor
workspace $laptopws5 output $laptopMonitor
workspace $laptopws6 output $laptopMonitor
workspace $laptopws7 output $laptopMonitor
workspace $laptopws8 output $laptopMonitor
workspace $laptopws9 output $laptopMonitor

workspace $mainws0 output $mainMonitor
workspace $mainws1 output $mainMonitor
workspace $mainws2 output $mainMonitor
workspace $mainws3 output $mainMonitor
workspace $mainws4 output $mainMonitor
workspace $mainws5 output $mainMonitor
workspace $mainws6 output $mainMonitor
workspace $mainws7 output $mainMonitor
workspace $mainws8 output $mainMonitor
workspace $mainws9 output $mainMonitor

workspace $sidews0 output $sideMonitor
workspace $sidews1 output $sideMonitor
workspace $sidews2 output $sideMonitor
workspace $sidews3 output $sideMonitor
workspace $sidews4 output $sideMonitor
workspace $sidews5 output $sideMonitor
workspace $sidews6 output $sideMonitor
workspace $sidews7 output $sideMonitor
workspace $sidews8 output $sideMonitor
workspace $sidews9 output $sideMonitor

bindsym $mod+0 workspace $laptopws0
bindsym $mod+1 workspace $laptopws1
bindsym $mod+2 workspace $laptopws2
bindsym $mod+3 workspace $laptopws3
bindsym $mod+4 workspace $laptopws4
bindsym $mod+5 workspace $laptopws5
bindsym $mod+6 workspace $laptopws6
bindsym $mod+7 workspace $laptopws7
bindsym $mod+8 workspace $laptopws8
bindsym $mod+9 workspace $laptopws9

bindsym $mod+Ctrl+0 workspace $mainws0
bindsym $mod+Ctrl+1 workspace $mainws1
bindsym $mod+Ctrl+2 workspace $mainws2
bindsym $mod+Ctrl+3 workspace $mainws3
bindsym $mod+Ctrl+4 workspace $mainws4
bindsym $mod+Ctrl+5 workspace $mainws5
bindsym $mod+Ctrl+6 workspace $mainws6
bindsym $mod+Ctrl+7 workspace $mainws7
bindsym $mod+Ctrl+8 workspace $mainws8
bindsym $mod+Ctrl+9 workspace $mainws9

bindsym $mod+$alt+0 workspace $sidews0
bindsym $mod+$alt+1 workspace $sidews1
bindsym $mod+$alt+2 workspace $sidews2
bindsym $mod+$alt+3 workspace $sidews3
bindsym $mod+$alt+4 workspace $sidews4
bindsym $mod+$alt+5 workspace $sidews5
bindsym $mod+$alt+6 workspace $sidews6
bindsym $mod+$alt+7 workspace $sidews7
bindsym $mod+$alt+8 workspace $sidews8
bindsym $mod+$alt+9 workspace $sidews9

# Move focused container to workspace
bindsym $mod+Shift+0 move container to workspace $laptopws0
bindsym $mod+Shift+1 move container to workspace $laptopws1
bindsym $mod+Shift+2 move container to workspace $laptopws2
bindsym $mod+Shift+3 move container to workspace $laptopws3
bindsym $mod+Shift+4 move container to workspace $laptopws4
bindsym $mod+Shift+5 move container to workspace $laptopws5
bindsym $mod+Shift+6 move container to workspace $laptopws6
bindsym $mod+Shift+7 move container to workspace $laptopws7
bindsym $mod+Shift+8 move container to workspace $laptopws8
bindsym $mod+Shift+9 move container to workspace $laptopws9

bindsym $mod+Shift+Ctrl+0 move container to workspace $mainws0
bindsym $mod+Shift+Ctrl+1 move container to workspace $mainws1
bindsym $mod+Shift+Ctrl+2 move container to workspace $mainws2
bindsym $mod+Shift+Ctrl+3 move container to workspace $mainws3
bindsym $mod+Shift+Ctrl+4 move container to workspace $mainws4
bindsym $mod+Shift+Ctrl+5 move container to workspace $mainws5
bindsym $mod+Shift+Ctrl+6 move container to workspace $mainws6
bindsym $mod+Shift+Ctrl+7 move container to workspace $mainws7
bindsym $mod+Shift+Ctrl+8 move container to workspace $mainws8
bindsym $mod+Shift+Ctrl+9 move container to workspace $mainws9

bindsym $mod+Shift+$alt+0 move container to workspace $sidews0
